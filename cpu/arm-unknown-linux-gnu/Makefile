.SUFFIXES: .cc
target = arm-unknown-linux-gnu
srcdir = .
VPATH = 
top_builddir    = ..
top_srcdir      = ../..

SHELL = /bin/sh



CXX = g++
CXXFLAGS = -g -O2 -D_FILE_OFFSET_BITS=64 -D_LARGE_FILES  $(X_CFLAGS)

LDFLAGS = 
LIBS =  -lm
X_LIBS = 
X_PRE_LIBS =  -lSM -lICE
RANLIB = ranlib



BX_INCDIRS = -I.. -I$(srcdir)/../.. -I../../instrument/stubs -I$(srcdir)/../../instrument/stubs

APIC_OBJS = 
EXT_DEBUG_OBJS = 

# Objects which are synced between the cpu and cpu64 code and
# are used for either compile.
OBJS = \
	cpu.o \
	init.o \
	debugstuff.o \
#	exception.o  
#	io_pro.o \
#	io.o \


BX_INCLUDES = ../../bochs.h ../../config.h


all: libcpu.a

.cc.o:
	$(CXX) -c $(BX_INCDIRS) $(CXXFLAGS) $< -o $@


libcpu.a: $(OBJS) 
	rm -f  libcpu.a
	ar rv $@ $(OBJS) 
	$(RANLIB) libcpu.a

$(OBJS): $(BX_INCLUDES)

clean:
	rm -f  *.o
	rm -f  *.a

dist-clean: clean
	rm -f  Makefile

